
post_id = <%= @post.id %>

$(`.bookmark_btn_${post_id}`).html(
'<%= escape_javascript(render partial:"unbookmark", locals:{bookmark: @bookmark})%>'
);

$('.message_container').fadeIn(300);
$('.message_wrap').html('<div class="bookmark_success"><div class="bookmark_icon"><%=icon "far","check-circle"%></div>ブックマークしました</div>');
setTimeout(function(){
  $('.message_container').fadeOut(300);
},1500);



delete post_id;